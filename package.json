{
  "name": "tg-file-manager",
  "version": "1.0.0",
  "description": "Telegram file management system",
  "main": "app.js",
  "scripts": {
    "build": "mkdir -p dist && cp -r public/* dist/ && cp app.js dist/ && cp package.json dist/  && cp -r routes dist/ && mkdir -p dist/functions && cp -r functions/* dist/functions/",
    "start": "node app.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "body-parser": "^1.20.2",
    "express-async-handler": "^1.2.0",
    "moment": "^2.29.4",
    "bcrypt": "^5.1.1",
    "express-session": "^1.18.1",
    "cookie-parser": "^1.4.7",
    "dotenv": "^10.0.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.6.7"
  },
  "engines": {
    "node": "18.x"
  }
}
